  "components": {
    "schemas": {
      "AddAccountRequest": {
        "required": [
          "currency"
        ],
        "type": "object",
        "properties": {
          "currency": {
            "maxLength": 3,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "BillType": {
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5,
          6,
          7,
          8
        ],
        "type": "integer",
        "format": "int32"
      },
      "ChangePasswordDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "currentPassword": {
            "type": "string",
            "nullable": true
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateBillRequest": {
        "required": [
          "amount",
          "billType",
          "dueDate"
        ],
        "type": "object",
        "properties": {
          "billType": {
            "$ref": "#/components/schemas/BillType"
          },
          "amount": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "dueDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "DepositRequest": {
        "required": [
          "amount"
        ],
        "type": "object",
        "properties": {
          "amount": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "description": {
            "maxLength": 250,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "PayBillRequest": {
        "required": [
          "accountId",
          "amount"
        ],
        "type": "object",
        "properties": {
          "accountId": {
            "type": "integer",
            "format": "int32"
          },
          "amount": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "RegisterRequest": {
        "required": [
          "birthDate",
          "confirmPassword",
          "email",
          "firstName",
          "lastName",
          "password",
          "phoneNumber"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string",
            "format": "tel"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string",
            "format": "password"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          },
          "birthDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "TransferRequest": {
        "required": [
          "amount",
          "receiverAccountNumber",
          "senderAccountId"
        ],
        "type": "object",
        "properties": {
          "receiverAccountNumber": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "amount": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "description": {
            "maxLength": 250,
            "type": "string",
            "nullable": true
          },
          "senderAccountId": {
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "TwoFactorRequest": {
        "required": [
          "isTwoFactorEnabled"
        ],
        "type": "object",
        "properties": {
          "isTwoFactorEnabled": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "UpdateUser": {
        "required": [
          "birthDate",
          "email",
          "firstName",
          "lastName",
          "phoneNumber"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "birthDate": {
            "type": "string",
            "format": "date"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string",
            "format": "tel"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "UserPatchDto": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "type": "string",
            "nullable": true
          },
          "phoneNumber": {
            "type": "string",
            "nullable": true
          },
          "istwoFactorEnable": {
            "type": "boolean"
          },
          "email": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "VerifyOtpRequest": {
        "required": [
          "otpCode",
          "userId"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "otpCode": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "JWT token giriniz.",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
